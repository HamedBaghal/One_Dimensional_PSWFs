    "from numpy import linalg\n",
    "import prolatematrix.ipynb as prolatematrix\n",
    "def coefficientprolate(c,m,n):\n",
    "    A=prolatematrix(c,m)\n",
    "    w,v=linalg.eig(A)\n",
    "    return v[:,0]"
